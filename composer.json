{
    "name": "asha23/arlo-framework-laravel-mix",
    "type": "project",
    "license": "MIT",
	"version":"1.0.4",
    "description": "A Composer Framework to help you get WordPress up and running in around a minute",
    "authors": [{
        "name": "Ash Whiting",
        "email": "ash_whiting@hotmail.com"
    }],
    "keywords": [
        "wordpress theme framework", "base theme", "wordpress", "composer", "webpack", "vagrant", "wp"
    ],
    "repositories": {
        "wp-packagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
		"arlo-framework-laravel-mix-seed-theme": {
            "type": "package",
            "package" : {
                "name":"asha23/arlo-framework-laravel-mix-seed-theme",
                "type":"wordpress-theme",
                "version":"1.0",
                "dist": {
                    "type":"zip",
                    "url":"https://github.com/asha23/arlo-framework-laravel-mix-seed-theme/archive/1.0.zip"
                }
            }
		},
		"core-functionality": {
            "type": "package",
            "package" : {
                "name":"asha23/core-functionality",
                "type":"wordpress-plugin",
                "version":"1.1.4",
                "dist": {
                    "type":"zip",
                    "url":"https://github.com/asha23/core-functionality/archive/1.1.4.zip"
                }
            }
		},
	    "wp-sync-db": {
			"type": "package",
			"package": {
				"name": "wp-sync-db/wp-sync-db",
				"type": "wordpress-plugin",
				"version": "1.6",
				"dist": {
					"type": "zip",
					"url": "https://github.com/corysimmons/wp-sync-db/archive/1.6.zip"
				}
			}
		}
    },
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0.12",
        "johnpbloch/wordpress": "*",
	    "vlucas/phpdotenv": "^2.0.1",
		"oscarotero/env": "^1.0",
		"asha23/core-functionality":"*",
        "wpackagist-plugin/wordpress-seo": "*",
		"wpackagist-plugin/disable-gutenberg":"*",
        "wp-sync-db/wp-sync-db": "*",
		"asha23/arlo-framework-laravel-mix-seed-theme":"*"
    },
    "extra": {
        "installer-paths": {
            "web/content/plugins/{$name}/": ["type:wordpress-plugin"],
            "web/content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "web/wp",
        "content-install-dir": "web/content"
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}